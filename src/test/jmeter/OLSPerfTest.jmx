        <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="GenerateRandomLatLon" enabled="true">
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">double minLat = Double.parseDouble(vars.get(&quot;minLat&quot;));
double requiredDeviation = Double.parseDouble(vars.get(&quot;maxLat&quot;)) - minLat;
double randomLat = minLat +  Math.random() * requiredDeviation;
vars.put(&quot;Lat&quot;, randomLat.toString());

double minLon = Double.parseDouble(vars.get(&quot;minLon&quot;)); 
requiredDeviation = Double.parseDouble(vars.get(&quot;maxLon&quot;)) - minLon;
double randomLon = minLon +  Math.random() * requiredDeviation;
vars.put(&quot;Lon&quot;, randomLon.toString());

return &quot;Lat:&quot; + randomLat + &quot;Lon:&quot; + randomLon ;</stringProp>
        </BeanShellPreProcessor>
